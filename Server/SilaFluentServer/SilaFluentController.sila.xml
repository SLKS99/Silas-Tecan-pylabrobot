<?xml version="1.0" encoding="utf-8"?>
<Feature xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" SiLA2Version="1.0" FeatureVersion="1.0" Originator="tecan" Category="fluent" xmlns="http://www.sila-standard.org">
  <Identifier>SilaFluentController</Identifier>
  <DisplayName>Fluent Service</DisplayName>
  <Description>Denotes a service to remote-control FluentControl</Description>
  <Command>
    <Identifier>AddLabware</Identifier>
    <DisplayName>Add Labware</DisplayName>
    <Description>Adds new labware to the worktable</Description>
    <Observable>No</Observable>
    <Parameter>
      <Identifier>Barcode</Identifier>
      <DisplayName>Barcode</DisplayName>
      <Description>the barcode of the labware</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>HasLid</Identifier>
      <DisplayName>Has Lid</DisplayName>
      <Description>True, if the labware has a lid</Description>
      <DataType>
        <Basic>Boolean</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>LabwareName</Identifier>
      <DisplayName>Labware Name</DisplayName>
      <Description>the name of the labware</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>LabwareType</Identifier>
      <DisplayName>Labware Type</DisplayName>
      <Description>the type of the labware</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>TargetLocation</Identifier>
      <DisplayName>Target Location</DisplayName>
      <Description>the location where the labware should be spawned</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>Position</Identifier>
      <DisplayName>Position</DisplayName>
      <Description>the position in the target location group</Description>
      <DataType>
        <Basic>Integer</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>Rotation</Identifier>
      <DisplayName>Rotation</DisplayName>
      <Description>the rotation of the labware</Description>
      <DataType>
        <Basic>Integer</Basic>
      </DataType>
    </Parameter>
    <DefinedExecutionErrors>
      <Identifier>NoExecutionChannel</Identifier>
      <Identifier>Runtime</Identifier>
    </DefinedExecutionErrors>
  </Command>
  <Command>
    <Identifier>RemoveLabware</Identifier>
    <DisplayName>Remove Labware</DisplayName>
